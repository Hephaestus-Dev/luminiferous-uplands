{
  "schemaVersion": 1,
  "id": "$modid",
  "version": "${version}",
  "name": "Luminiferous Uplands",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Liliana \"RoboSky\"",
    "Vivian \"PrismaticVoid\" Jones",
    "Mya \"Nofacednerd\" Katz",
    "Thalia \"kwerti\""
  ],
  "license": "MIT",
  "icon": "assets/luminiferous_uplands/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "scala",
        "value": "robosky.ether.UplandsMod"
      }
    ],
    "rei_plugins": [
      "robosky.ether.interop.EtherREIPlugin"
    ]
  },
  "mixins": [
    "luminiferous_uplands.mixins.json",
    "dimension_fixes.mixins.json",
    {
      "config": "luminiferous_uplands.client-mixins.json",
      "environment": "client"
    }
  ],
  "custom": {
    "extrapieces:piecepack": "extrapieces.luminiferous_uplands.json"
  },
  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": "*",
    "fabric-language-scala": "*",
    "libgui": "*"
  }
}
