{
  "schemaVersion": 1,
  "id": "$modid",
  "version": "${version}",
  "name": "Luminiferous Uplands",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "RoboSky"
  ],
  "license": "EPL",
  "icon": "assets/luminiferous_uplands/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "scala",
        "value": "robosky.ether.UplandsMod"
      }
    ]
  },
  "mixins": [
    "luminiferous_uplands.mixins.json",
    "dimension_fixes.mixins.json",
    {
      "config": "luminiferous_uplands.client-mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": "*",
    "fabric-language-scala": "*",
    "libgui": "*"
  }
}